<?xml version="1.0"?>
<openerp>
  <data>
    <record id="product_manager_form_view" model="ir.ui.view">
      <field name="name">product.product.form.manager</field>
      <field name="model">product.product</field>
      <field name="inherit_id" ref="product.product_normal_form_view"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='categ_id']" position="before">
          <field name="product_manager" string="Manager"/>
        </xpath>
      </field>
    </record>
  </data>
</openerp>
